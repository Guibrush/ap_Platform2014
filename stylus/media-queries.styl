@media only screen and (min-width: 40.063em)
// 641px
	
	// Utilities
	.u-buttonStrong, .u-buttonLight-white
		width auto

	// End Utilities

	// Features

	.featureImage-desk
		display inherit

	.featureImage-mobile
		display none
		
	.mainFeature-data
		display block
		span
			font-size 7em
	
	.mainFeature
		a
			display none

	// End Features
		
	// Home
	
		
	.homeMain-text
		max-width 60%
				
	.homeEmailing-text
		margin-top 5%
		
	.buttonHome-link
		background white
		border 2px solid orange
		color orange
		display inline-block
		margin 1em 0
		padding 1em
		text-align center
		text-decoration none
		transition .3s
		width 15em
		&:hover
			background orange
			border 2px solid orange
			color white
	.buttonHome
		width 15em
	
	.homeFeature-push
		img
			width 70%
	
	// End Home
		
	// Landing Publicidad

	.publicidadFeatureBackground-desk
		background-image url(../img/publicidad/decide-desktop.png)
		display inherit
		margin-bottom 2em
		margin-top 2em
		overflow hidden
		h2
			font-size 3.5em
			color grey-dark
		
	.publicidadFeatureBackground-mobile
		display none
		

	.publicidadFeature-banner
		padding 0 4.5em
		
	// End Landing Publicidad
	
	// Landing Pagina-promocional
		
	.paginaPromocional-mainImg, .paginaPromocionalPlanes-mobile
		display none

	.paginaPromocionalPlanes-desk
		background-image url(../img/pagina-promocional/mantenimiento-desktop.png)
		display inherit

	#paginaPromocionalMain-figure
		img
			max-width 60%

	// End Landing Pagina-promocional
		
	// Landing Emailing

	#emailingMain-figure-mobile
		display none
		
	#emailingMain-figure-tablet
		display inherit
		
	.emailingMensaje-mobile
		display none

	// End Landing Emailing
		
	// Landing Redes
		
	#redesMain-figure-mobile
		display none

	#redesMain-figure-tablet
		display inherit
		
	// End Landing Redes
		
	// Comunidad Home
		
	.comunidadHome-promocionateText
		margin-top 15%

	// End Comunidad Home
		
	// Comunidad Apuntate
	.comunidadApuntate
		background-image url(../img/comunidad/fondo.png)
		padding-top 5em	

	.comunidadApuntate-pushMain
		display inherit
		
	.comunidadApuntate-pushMain-content
		background orange
		border-radius 50%
		height 170px
		margin 1em auto
		padding 2em 1em
		width 170px
		.icon-comunidadApuntate	
			display block
			font-size 3em
			margin 0
			padding 0
		.comunidadApuntate-iconText
			color white
			display block
			font-size .8em
			font-weight normal
			line-height 1.2em
			margin-top 2em
	
	.comunidadApuntate-text
		background rgba(255, 255, 255, 0.56)
		max-width 90%
		padding 1em 3em
		text-align center
		p
			text-align center
		
	.comunidadApuntate-form		
		max-width 90%
		
	.comunidadApuntate-button
		p
			font-size 1.5em
		
	// End Comunidad Apuntate
		
	// Pricing
	
	.u-buttonStrong-small
		width 100%
		
	#pricingClose1, #pricingClose2, #pricingClose3, #pricingOpen1, #pricingOpen2, #pricingOpen3
		display initial
	
	.pricingMobile
		cursor default
	
	.pricingClose
		background grey-legal
		border 1px solid grey-legal
		h4, span, p
			color black !important
			
	.pricingClose-emailing
		background grey-dark
		padding 1em 2em
		h4, span, p
			color white !important
			line-height 40px
			margin-bottom 0
		h4
			font-family "Roboto", Arial, sans-serif
			font-size 1.5em
			font-style normal
			font-weight 300
			margin 0
			text-rendering optimizeLegibility
	
	.pricingDestacado
		background grey-legal
		p
			color grey-dark
	
			
	.priceIcon
		display none
	
	// End Pricing

	// Menu
		
	#icon-close
		padding 1.1em
		img
			width 2em

	#Menu-logo-mobile
		padding 1.4em
		img
			width 9em
		
	#icon-menu
		margin 1.2em
		img
			width 2.5em

	#Logo-mobile
		padding 1.2em
		img
			width 11em
	
	// End Menu
		
	// Crosseling
	
	.crosseling
		article
			padding 2em
			
	// End Crosseling

		
	// Footer		
	
	// End Footer
	
